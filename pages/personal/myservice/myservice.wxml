<!--pages/personal/myservice/myservice.wxml-->
<block wx:for="{{sermens}}" wx:key="index">
  <view class='fcard card' bindtap='clickFuwujia'>
    <view class='remarks'>
      <view class='icon'>
        <image src="{{item.userasset}}" alt="" />
        <text class='name'>{{item.name}}</text>
      </view>
      <view class='labels'>
        <view class='label'>洗衣</view>
        <view class='label'>做饭</view>
        <view class='label'>早教</view>
      </view>
    </view>
  </view>
</block>